<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    android:backgroundTint="@color/white"
    tools:context=".CountriesPage">

    <View
        android:id="@+id/divider"
        android:layout_width="450dp"
        android:layout_height="56dp"
        android:background="@color/background"
        android:backgroundTint="@color/background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/frameLayout2" />

    <Button
        android:id="@+id/button7"
        android:layout_width="410dp"
        android:layout_height="89dp"
        android:layout_marginTop="140dp"
        android:layout_weight="1"
        android:height="60dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/flag_14821533"
        android:drawablePadding="0dp"
        android:gravity="left|center"
        android:onClick="GermanyCities"
        android:padding="10dp"
        android:text="Германия"
        android:textColor="#000"
        android:textSize="35sp"
        android:textStyle="normal"
        app:cornerRadius="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:layout_width="410dp"
        android:layout_height="89dp"
        android:layout_marginTop="316dp"
        android:layout_weight="1"
        android:height="60dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/france_206657"
        android:drawablePadding="0dp"
        android:gravity="left|center"
        android:onClick="FranceCities"
        android:padding="10dp"
        android:text="Франция"
        android:textColor="#000"
        android:textSize="35sp"
        android:textStyle="normal"
        app:cornerRadius="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:layout_width="410dp"
        android:layout_height="89dp"
        android:layout_marginTop="404dp"
        android:layout_weight="1"
        android:height="60dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/turkey_2151383"
        android:drawablePadding="0dp"
        android:gravity="left|center"
        android:padding="10dp"
        android:text="Турция"
        android:textColor="#000"
        android:textSize="35sp"
        android:textStyle="normal"
        app:cornerRadius="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="410dp"
        android:layout_height="89dp"
        android:layout_marginTop="492dp"
        android:layout_weight="1"
        android:height="60dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/italy_3373278"
        android:drawablePadding="0dp"
        android:gravity="left|center"
        android:padding="10dp"
        android:text="Италия"
        android:textColor="#000"
        android:textSize="35sp"
        android:textStyle="normal"
        app:cornerRadius="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="410dp"
        android:layout_height="89dp"
        android:layout_marginTop="580dp"
        android:layout_weight="1"
        android:height="60dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/greece_555608"
        android:drawablePadding="0dp"
        android:gravity="left|center"
        android:padding="10dp"
        android:text="Греция"
        android:textColor="#000"
        android:textSize="35sp"
        android:textStyle="normal"
        app:cornerRadius="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button6"
        android:layout_width="410dp"
        android:layout_height="89dp"
        android:layout_marginTop="52dp"
        android:layout_weight="1"
        android:height="60dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/russia_555451"
        android:drawablePadding="0dp"
        android:gravity="left|center"
        android:onClick="RussiaCities"
        android:padding="10dp"
        android:text="Россия"
        android:textColor="#000"
        android:textSize="35sp"
        android:textStyle="normal"
        app:cornerRadius="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:layout_width="410dp"
        android:layout_height="89dp"
        android:layout_marginTop="228dp"
        android:layout_weight="1"
        android:height="60dp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/spain_206724"
        android:drawablePadding="0dp"
        android:gravity="left|center"
        android:onClick="SpainCities"
        android:padding="10dp"
        android:text="Испания"
        android:textColor="#000"
        android:textSize="35sp"
        android:textStyle="normal"
        app:cornerRadius="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="@color/white"
        android:onClick="MainPage"
        android:text="Назад"
        android:textColor="@color/background"
        app:cornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="@color/stroke"
        app:strokeWidth="1dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/abhaya_libre_bold"
        android:text="Выберите страну"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.77"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView2"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_navmenu">

    </com.google.android.material.bottomnavigation.BottomNavigationView>

</androidx.constraintlayout.widget.ConstraintLayout>